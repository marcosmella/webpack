    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.5.0",
    "nib": "^1.1.2",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.14",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "rupture": "^0.7.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
